{
  "evaluation": {
    "source": {
      "name": "primaryid",
      "display": "source",
      "aggregation": "COUNT_DISTINCT"
    },
    "comparison_operator": "GREATER_THAN_OR_EQUAL",
    "threshold": {
      "value": {
        "double_value": 1.0
      }
    },
    "notification": {
      "subscriptions": [
          {
            "user_email": "kisarawenu@gmail.com"
          }
        ]
    }
  },
  "schedule": {
    "quartz_cron_schedule": "14 50 23 * * ?",
    "timezone_id": "Asia/Colombo"
  },
  "query_lines": [
    "with base as (",
    "  select ",
    "    * except(fda_dt, init_fda_dt), ",
    "    to_date(init_fda_dt, 'yyyyMMdd') as init_fda_dt,",
    "    to_date(fda_dt, 'yyyyMMdd') as fda_dt",
    "  from pediatric_adr_events.dbt_transforms_intermediate.int__demo_base",
    ")",
    "",
    "select *",
    "from base",
    "where fda_dt >= '2025-09-30'",
    "and init_fda_dt <> fda_dt",
    "and origin_country <> 'US'",
    "and sex = 'M';"
  ]
}
